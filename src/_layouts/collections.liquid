---
layout: default
---
{% rendercontent "_v2/content", emoji: page.emoji, title: page.title %}
  {% with links %}
    {%- for backlink in page.backlinks -%}
      {% render "_v2/backlink", backlink: backlink %}
    {%- endfor -%}
  {% endwith %}
  {% with list %}
    {{ content }}
  {% endwith %}
{% endrendercontent %}
