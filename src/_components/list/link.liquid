---
name: List Link
description: description
variables:
  name: string
  url: string
  description: string
---

<div class="flex flex-col space-y-1">
  <a
    class="text-blue-600 dark:text-blue-500"
    href="{{ url }}"
    target="_blank"
    rel="noopener noreferrer"
  >
    {{ name }}
  </a>
  <p>{{ description }}</p>
</div>
